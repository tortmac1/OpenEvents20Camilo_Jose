<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.openevents20">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OpenEvents20"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".Actividades.UserInfo"
            android:exported="false" />
        <activity
            android:name=".Actividades.EditMyUser"
            android:exported="false" />
        <activity
            android:name=".Actividades.CreateEvent"
            android:exported="false" />
        <activity
            android:name=".Actividades.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Actividades.EventInfo"
            android:exported="false" />
        <activity
            android:name=".Actividades.MenuFragments"
            android:exported="false" />
        <activity
            android:name=".Actividades.Login"
            android:exported="false" />
        <activity
            android:name=".Actividades.Register"
            android:exported="false" />
    </application>

</manifest>